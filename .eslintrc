{
    "parser": "babel-eslint",
    
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "globals": {
        "window": true,
        "document": true
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["./src"],
                "extensions": [".js"]
            }
        }
    },
    "rules": {
        "strict": 0,
        "react/jsx-filename-extension": 0
      }
  }